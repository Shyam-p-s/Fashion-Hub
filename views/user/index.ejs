    <% if(locals.bannerData) {%>

      

      <div id="carouselExample" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <% bannerData.forEach((banner, index) => { %>
            <li data-target="#carouselExample" data-slide-to="<%= index %>" class="<%= index === 0 ? 'active' : '' %>"></li>
          <% }) %>
        </ol>
        <div class="carousel-inner">
          <% bannerData.forEach((banner, index) => { %>
          
            <div class="carousel-item <%= index === 0 ? 'active' : '' %>">
              <div class="site-blocks-cover" style="background-image: url(<%= (banner.image[0]) %>);" data-aos="fade">

                <div class="container">
                  <div class="row align-items-start align-items-md-center justify-content-end">
                    <div class="col-md-5 text-center text-md-left pt-5 pt-md-0">
                      <h1 class="mb-2" style="color: #fff;"><%= banner.heading %></h1>
                      <div class="intro-text text-center text-md-left">
                        <p class="mb-4" style="color: #E8F2F0;" ><%= banner.subHeading %></p>
                        <p>
                          <a href="/shop" class="btn btn-sm btn-primary">Shop Now</a>
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          
          <% }) %>
        </div>
        <a class="carousel-control-prev" href="#carouselExample" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExample" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
      

    <%}%>
    
    

